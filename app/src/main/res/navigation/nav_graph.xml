<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/first_fragment">

    <action android:id="@+id/action_global_first_fragment"
        app:destination="@id/first_fragment"/>

    <fragment
        android:id="@+id/first_fragment"
        android:name="com.example.teentrega.ui.fragment.CreateAccountFirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_create_account_first">
        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/second_fragment" />
    </fragment>

    <fragment
        android:id="@+id/second_fragment"
        android:name="com.example.teentrega.ui.fragment.CreateAccountSecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_create_account_second">
    </fragment>
</navigation>